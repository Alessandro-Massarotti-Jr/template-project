{
  "name": "template-project",
  "version": "1.0.0",
  "main": "index.js",
  "description": "template project for typescript projects",
  "keywords": [
    "template"
  ],
  "homepage": "https://github.com/Alessandro-Massarotti-Jr/template-project",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alessandro-Massarotti-Jr/template-project"
  },
  "license": "MIT",
  "author": {
    "name": "Alessandro Massarotti Junior",
    "email": "alemassajr@hotmail.com"
  },
  "engines": {
    "node": ">=20.3.1",
    "npm": ">=9.1.1"
  },
  "contributors": [],
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/index.ts",
    "build": "npx tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "node build/index.js",
    "prepare": "husky",
    "test": "jest --passWithNoTests --no-cache --silent",
    "test-coverage": "jest --coverageReporters=text --passWithNoTests --no-cache --silent",
    "precommit": "npx husky install",
    "commit": "cz",
    "lint": "eslint src/**/*.{ts,tsx}",
    "fix": "prettier . --write eslint src/**/*.{ts,tsx} --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/bcrypt": "^5.0.2",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/moment": "^2.13.0",
    "@types/nodemailer": "^6.4.15",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@swc/core": "^1.3.21",
    "@swc/jest": "^0.2.23",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^4.16.2",
    "prisma-erd-generator": "^1.11.2",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "bcrypt": "^5.1.1",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "nodemailer": "^6.9.13",
    "uuid": "^9.0.1",
    "uuidv4": "^6.2.13",
    "zod": "^3.23.6"
  }
}
