{
  "name": "template-project",
  "version": "1.1.8",
  "main": "index.js",
  "description": "template project for typescript projects",
  "keywords": [
    "template",
    "node.js",
    "typescript"
  ],
  "homepage": "https://github.com/Alessandro-Massarotti-Jr/template-project",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alessandro-Massarotti-Jr/template-project"
  },
  "license": "MIT",
  "author": {
    "name": "Alessandro Massarotti Junior",
    "email": "alemassajr@hotmail.com"
  },
  "engines": {
    "node": ">=22.6.0",
    "npm": ">=9.1.1"
  },
  "contributors": [],
  "scripts": {
    "dev": "npx ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn --env-file=.env src/index.ts",
    "prebuild": "rm -rf ./build",
    "build": "npx tsc --project tsconfig.json",
    "start": "node src/index.js",
    "test": "jest --passWithNoTests --no-cache --silent --setTimeout=45000",
    "test-coverage": "jest --coverageReporters=text --passWithNoTests --no-cache --silent --setTimeout=45000",
    "precommit": "npx husky",
    "commit": "cz",
    "lint": "eslint src/**/*.{ts,tsx}",
    "fix": "prettier . --write eslint src/**/*.{ts,tsx} --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@swc/core": "^1.7.26",
    "@swc/jest": "^0.2.36",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "cors": "^2.8.5",
    "express": "^4.21.0"
  }
}